,"column":5}},{"generated":{"line":135,"column":14},"source":"Components/Host.js","original":{"line":80,"column":0}},{"generated":{"line":135,"column":17},"source":"Components/Host.js","original":{"line":80,"column":17}},{"name":"player","generated":{"line":136,"column":0},"source":"Components/Host.js","original":{"line":81,"column":2}},{"name":"player","generated":{"line":136,"column":2},"source":"Components/Host.js","original":{"line":81,"column":2}},{"generated":{"line":136,"column":8},"source":"Components/Host.js","original":{"line":81,"column":8}},{"name":"PropTypes","generated":{"line":136,"column":10},"source":"Components/Host.js","original":{"line":81,"column":10}},{"name":"object","generated":{"line":136,"column":29},"source":"Components/Host.js","original":{"line":81,"column":20}},{"generated":{"line":136,"column":35},"source":"Components/Host.js","original":{"line":81,"column":10}},{"name":"isRequired","generated":{"line":136,"column":36},"source":"Components/Host.js","original":{"line":81,"column":27}},{"generated":{"line":137,"column":0},"source":"Components/Host.js","original":{"line":80,"column":17}},{"generated":{"line":137,"column":1},"source":"Components/Host.js","original":{"line":80,"column":0}},{"name":"Host","generated":{"line":138,"column":15},"source":"Components/Host.js","original":{"line":84,"column":15}}],"sources":{"Components/Host.js":"/**\n * Host.js\n */\nimport React from 'react';\nimport PropTypes from 'prop-types';\n\nimport { emitHostPlayerStateChanged, connectToListener } from '../SocketClient';\n\nclass Host extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      isLoading: true,\n    };\n    this.player = props.player;\n  }\n\n  componentDidMount() {\n    console.log(this.player);\n    if (this.player) {\n      this.addEventHandlers();\n    }\n  }\n\n  componentDidUpdate(prevProps, nextProps) {\n    if (prevProps.player !== nextProps.player) {\n      if (nextProps.player) this.addEventHandlers();\n    }\n  }\n\n  addEventHandlers = () => {\n    console.log('Adding event handlers');\n    let currentState;\n    connectToListener(currentState);\n\n    // Error handling\n    this.player.addListener('initialization_error', ({ message }) => {\n      console.error(message);\n    });\n    this.player.addListener('authentication_error', ({ message }) => {\n      console.error(message);\n    });\n    this.player.addListener('account_error', ({ message }) => {\n      console.error(message);\n    });\n    this.player.addListener('playback_error', ({ message }) => {\n      console.error(message);\n    });\n    // Playback status updates\n    this.player.addListener('player_state_changed', (state) => {\n      currentState = state;\n      emitHostPlayerStateChanged(state);\n    });\n    // Ready\n    this.player.addListener('ready', ({ device_id }) => {\n      console.log('Ready with Device ID', device_id);\n      this.setState({\n        isLoading: false,\n      });\n    });\n\n    // Not Ready\n    this.player.addListener('not_ready', ({ device_id }) => {\n      console.log('Device ID has gone offline', device_id);\n    });\n    // Connect to the player!\n    this.player.connect();\n  };\n\n  render() {\n    const { isLoading } = this.state;\n    return (\n      <div className=\"Host\">\n        {isLoading ? <h1>Spotify is loading</h1> : <h1>You are hosting the session</h1>}\n      </div>\n    );\n  }\n}\n\nHost.propTypes = {\n  player: PropTypes.object.isRequired,\n};\n\nexport default Host;\n"},"lineCount":null}},"hash":"86ec5962efe9b952efb643db639e8ece","cacheData":{"env":{}}}