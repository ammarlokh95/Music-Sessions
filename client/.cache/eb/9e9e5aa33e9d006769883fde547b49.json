d":{"line":79,"column":1},"source":"Actions/PopulateDisplay.js","original":{"line":33,"column":7}},{"name":"baseURL","generated":{"line":93,"column":26},"source":"Actions/PopulateDisplay.js","original":{"line":3,"column":6}},{"name":"getUsersSongs","generated":{"line":94,"column":26},"source":"Actions/PopulateDisplay.js","original":{"line":5,"column":13}},{"name":"getUsersAlbums","generated":{"line":95,"column":26},"source":"Actions/PopulateDisplay.js","original":{"line":33,"column":13}}],"sources":{"Actions/PopulateDisplay.js":"import { DISPLAY_USERS_SONGS, DISPLAY_USERS_ALBUMS, FETCH_ERROR } from './types';\n\nconst baseURL = 'https://api.spotify.com/v1';\n\nexport const getUsersSongs = () => (dispatch) => {\n  const URL = `${baseURL}/me/tracks`;\n  const { token } = window;\n  console.log(`fetching songs : ${token}`);\n  if (token) {\n    fetch(URL, {\n      method: 'GET',\n      headers: {\n        Authorization: `Bearer ${token}`,\n      },\n    })\n      .then(resp => resp.json())\n      .then((tracks) => {\n        dispatch({\n          type: DISPLAY_USERS_SONGS,\n          payload: tracks,\n        });\n      })\n      .catch((err) => {\n        console.log(err);\n        dispatch({\n          type: FETCH_ERROR,\n          payload: err,\n        });\n      });\n  }\n};\n\nexport const getUsersAlbums = () => (dispatch) => {\n  const URL = `${baseURL}/me/albums`;\n  const { token } = window;\n  if (token) {\n    fetch(URL, {\n      method: 'GET',\n      headers: {\n        Authorization: `Bearer ${token}`,\n      },\n    })\n      .then(resp => resp.json())\n      .then((albums) => {\n        dispatch({\n          type: DISPLAY_USERS_ALBUMS,\n          payload: albums,\n        });\n      })\n      .catch((err) => {\n        console.log(err);\n        dispatch({\n          type: FETCH_ERROR,\n          payload: err,\n        });\n      });\n  }\n};\n"},"lineCount":null}},"hash":"b0c8b66bfc549b12410ab73975b30c52","cacheData":{"env":{}}}