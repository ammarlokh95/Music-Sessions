erated":{"line":150,"column":29},"source":"Container/Listener.js","original":{"line":81,"column":20}},{"generated":{"line":150,"column":35},"source":"Container/Listener.js","original":{"line":81,"column":10}},{"name":"isRequired","generated":{"line":150,"column":36},"source":"Container/Listener.js","original":{"line":81,"column":27}},{"generated":{"line":151,"column":0},"source":"Container/Listener.js","original":{"line":80,"column":21}},{"generated":{"line":151,"column":1},"source":"Container/Listener.js","original":{"line":80,"column":0}},{"name":"Listener","generated":{"line":152,"column":15},"source":"Container/Listener.js","original":{"line":84,"column":15}},{"name":"Listener","generated":{"line":166,"column":26},"source":"Container/Listener.js","original":{"line":9,"column":6}}],"sources":{"Container/Listener.js":"/**\n * Listener.js\n */\nimport React from 'react';\nimport PropTypes from 'prop-types';\n\nimport { onHostPlayerStateChanged } from '../SocketClient';\n\nclass Listener extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      isLoading: true,\n    };\n    this.player = props.player;\n  }\n\n  componentDidMount() {\n    console.log(this.player);\n    if (this.player) {\n      this.addEventHandlers();\n    }\n  }\n\n  componentDidUpdate(prevProps, nextProps) {\n    if (prevProps.player !== nextProps.player) {\n      if (nextProps.player) this.addEventHandlers();\n    }\n  }\n\n  addEventHandlers = () => {\n    console.log('Adding event handlers');\n    // Error handling\n    this.player.addListener('initialization_error', ({ message }) => {\n      console.error(message);\n    });\n    this.player.addListener('authentication_error', ({ message }) => {\n      console.error(message);\n    });\n    this.player.addListener('account_error', ({ message }) => {\n      console.error(message);\n    });\n    this.player.addListener('playback_error', ({ message }) => {\n      console.error(message);\n    });\n    // Playback status updates\n    this.player.addListener('player_state_changed', (state) => {\n      console.log(state);\n    });\n    // Ready\n    this.player.addListener('ready', ({ device_id }) => {\n      console.log('Ready with Device ID', device_id);\n      this.setState({\n        isLoading: false,\n      });\n    });\n\n    // recieve playback change\n    onHostPlayerStateChanged((err, host_player) => {\n      console.log(host_player);\n    });\n    // Not Ready\n    this.player.addListener('not_ready', ({ device_id }) => {\n      console.log('Device ID has gone offline', device_id);\n    });\n    // Connect to the player!\n    this.player.connect();\n  };\n\n  render() {\n    const { isLoading } = this.state;\n    return (\n      <div className=\"Listener\">\n        {isLoading ? <h1>Spotify is loading</h1> : <h1>You are listening</h1>}\n      </div>\n    );\n  }\n}\n\nListener.propTypes = {\n  player: PropTypes.object.isRequired,\n};\n\nexport default Listener;\n"},"lineCount":null}},"hash":"5c33e73630ba7ed47d19d7e2e2a32d73","cacheData":{"env":{}}}